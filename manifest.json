{
  "name": "Meeting Wingman",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "login.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["auto-trigger.js"],
    "persistent": false
  }
}
